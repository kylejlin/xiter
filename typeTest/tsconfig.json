{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "downlevelIteration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "declaration": true
  },
  "include": ["./*.d.ts", "./*.test-d.ts", "test.ts"]
}
